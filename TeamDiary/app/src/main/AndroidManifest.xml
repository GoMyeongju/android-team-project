<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamdiary">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity"
            android:label="회원가입"/>
        <activity android:name=".TeamSelectActivity"
            android:label="팀 선택"/>
        <activity android:name=".TeamAddActivity"
            android:label="팀 추가"/>
        <activity android:name=".ProjectSelectActivity"
            android:label="프로젝트 선택"/>
        <activity android:name=".ProjectAddActivity"
            android:label="프로젝트 추가"/>
        <activity android:name=".MainContentsActivity" />
        <activity android:name=".LoadingActivity"/>
        <activity android:name=".CalendarActivity"/>
        <activity android:name=".MemberInfoActivity"/>
        <activity android:name=".TopicInfo"/>
        <activity android:name=".TopicAddActivity"/>
        <activity android:name=".TopicListActivity"/>
    </application>

</manifest>